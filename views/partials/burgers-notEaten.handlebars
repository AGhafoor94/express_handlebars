{{#unless eaten}}
<div class="card ">
    <div class="card-content rightBorder">
        <li class="burgerList">
            <p class="subtitle name">{{burger_name}}</p>
            <footer class="card-footer">
                <button onClick="updateItem(this)" class="button is-warning eatBtn reloadButton" data-id="{{id}}"
                    data-eaten="{{eaten}}">Eat</button>
            </footer>
        </li>
    </div>
</div>
{{/unless}}