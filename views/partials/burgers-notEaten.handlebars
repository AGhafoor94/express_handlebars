{{#unless eaten}}
<li class="burgerList">
    <span class="subtitle name">{{burger_name}}</span>
    <form action="/api/burgers" method="PUT">
        <button onClick="updateItem(this)" type="submit" class="button is-warning eatBtn reloadButton" data-id="{{id}}"
            data-eaten="{{eaten}}">Eat</button>
    </form>
</li>
{{/unless}}